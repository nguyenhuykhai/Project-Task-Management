{
  "name": "micro-fe-skeleton",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@9.15.0",
  "scripts": {
    "dev": "pnpm --parallel --filter \"@repo/*\" dev",
    "dev:mfe1": "pnpm --filter @repo/mfe1 dev",
    "dev:mfe2": "pnpm --filter @repo/mfe2 dev",
    "dev:docs": "pnpm --filter @repo/docs dev",
    "predev": "pnpm stop",
    "stop": "kill-port 3000 3001 3002",
    "preview": "pnpm --parallel --filter \"@repo/*\" preview",
    "build": "pnpm --recursive --filter \"@repo/*\" build",
    "build:mfe1": "pnpm --filter @repo/mfe1 build",
    "build:mfe2": "pnpm --filter @repo/mfe2 build",
    "build:packages": "pnpm --filter \"./packages/*\" build",
    "clean": "pnpm --recursive exec rm -rf node_modules dist .turbo",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "typecheck": "pnpm --recursive --filter \"@repo/*\" typecheck"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "kill-port": "^2.0.1",
    "prettier": "^3.7.4",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  }
}
